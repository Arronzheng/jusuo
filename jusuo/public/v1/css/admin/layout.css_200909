/*多选父子树样式*/
.checkbox-tree-list{}
.checkbox-tree-list .checkbox-block{overflow:hidden;margin-bottom:15px;}
.checkbox-tree-list .checkbox-block .checkbox-parent{float:left;font-weight:bold;width:150px;}
.checkbox-tree-list .checkbox-block .checkbox-child{float:left;width:700px;}
.checkbox-tree-list .checkbox-block .checkbox-child span{color:#999999;}


/*百度地图导航样式*/
#baidu-map {width: 100%;height: 100%;overflow: hidden;margin:0;font-family:"微软雅黑";}
#baidu-map img{max-height:none;max-width:none;width: auto;}
#map-result{
	overflow-y: scroll;
	color:#000000;margin-left:10px;
}
#map-result .block{padding:10px 0;padding-left:30px;position:relative;}
#map-result .block .selected{
	display:none;
	width: 24px;
	height: 24px;
	position: absolute;
	top: 7px;
	left: 0px;
	z-index:10;
	background-color:#ffffff;
}
#map-result .block .unselect{
	width: 23px;
	height: 23px;
	position: absolute;
	top: 7px;
	left: 0px;
	z-index:9;
}
#map-result .block:hover{cursor:pointer}
#map-result .title{
	color:#3E82F7;margin-bottom:5px;
}
#map-result .address{
	color:#555555;
}

