.sub-nav-panel{
	position: absolute;
	padding: 10px;
	line-height: 40px;
	background-color: #fff;
	width: 140px;
	z-index: 1;
	top: -10px;
	left: 140px;
	display: none;
}

.sub-nav-panel a{
	cursor: pointer;
	/*margin-right: 20px;*/
}
.sub-nav-panel a:hover{
	color:#3D82F7;
}
.hidden{
	display: none;
}
.layui-side.layui-bg-black{
	transition: width 0.3s;
}
.layui-side.layui-bg-black.active, .layui-side.layui-bg-black.active>.layui-side-scroll{
	width: 320px;
}
.layui-side.layui-bg-black dd:hover > .sub-nav-panel{
	display: block;
}
.layui-side.layui-bg-black.active .layui-side-scroll {
	box-shadow: 5px 0 5px #F5F6FA;
}
.sub-nav-panel a.title{
	color: #555555;
	position: relative;
	padding-left: 12px;
}
.sub-nav-panel div.title{
	font-weight: bold;
	color: #555555;
	position: relative;
	padding-left: 12px;
}
.sub-nav-panel div.title:before{
	content: '';
	height: 12px;
	width: 3px;
	background-color: #555555;
	position: absolute;
	top: 14px;
	left: 0;
}
.layui-side-scroll::-webkit-scrollbar {
	width: 1px;
	height: 1px;
}
.layui-side-scroll::-webkit-scrollbar-thumb {
	border-radius: 1px;
	background: #808080;
}
.layui-side-scroll::-webkit-scrollbar-track {
	border-radius: 1px;
	background: #F5F6FA;
}

/*表单颜色全部换为蓝色*/
/*checkbox*/
.layui-form-checked[lay-skin=primary] i{
	border-color: #3E82F7!important;
	background-color: #3E82F7!important;
}
.layui-form-checkbox span {
	/*height: 100%; //这个值有问题，所以注释掉*/
	height:auto!important;
}
.layui-form-checkbox[lay-skin=primary]:hover i{border-color:#3E82F7;color:#fff}


/*radio*/
.layui-form-radio>i:hover, .layui-form-radioed>i{
	color:#3D82F7;
}


/*layui date*/
.layui-laydate .layui-this{
	background-color:#3D82F7 !important;
}


/*select*/
.layui-form-select dl dd.layui-this{
	background-color:#3D82F7;
}


/*文件上传*/
.layui-upload-drag{  /*文件拖拽上传框*/
	line-height:1.3;
	padding:20px 30px;
	position:relative;
}
.layui-upload-drag .upload-img-preview {
	position: absolute;
	top: 0;
	left: 0;
	height: 100%;
	width: 100%;
	background-size: cover;
	background-position: 50% 50%;
}
.layui-upload-drag .layui-icon{
	color: #3E82F7;
}

/*dropdown*/
.layui-dropdown-toggle{line-height:20px;}
.layui-dropdown-menu{
	box-shadow: 0 2px 4px rgba(0,0,0,.12);
	border: 1px solid #d2d2d2;
	background-color: #fff;
}
.layui-dropdown-menu .menu-item{display:block;color:#333333;text-align:center;line-height:35px;}
.layui-dropdown-menu .menu-item:hover{
	background-color: #f2f2f2;
	color: #000;
	cursor: pointer;
}
