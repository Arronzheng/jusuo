/*框架->底部*/
.layui-layout-admin .layui-footer{
	background-color:#ffffff;
}

/*框架->顶部*/
.layui-layout-admin .layui-header{
	background-color:#ffffff;
	border-bottom:1px solid #E7EBF4;
}
.layui-layout-admin .layui-header .layui-layout-left .header-title{
	color:#ffffff;
	font-weight:bold;
	line-height:50px;
}
.layui-nav .layui-nav-item{
	line-height: 40px;
}
.layui-nav-child{
	top: 40px;
	line-height: 40px;
	padding: 0 0;
}
.side-nav-group-title{
	position: relative;
	height: 45px;
	line-height: 45px;
	text-overflow: ellipsis;
	overflow: hidden;
	white-space: nowrap;
	display: block;
	padding: 0 20px;
	transition: all .3s;
	-webkit-transition: all .3s;
	color:#333333!important;font-weight:bold;
}
.layui-layout-admin .layui-nav .layui-this:after, .layui-nav-bar, .c-side-menu .layui-nav-itemed:after{
	/*不知名色块*/
	background-color:#3D82F7;
	display:none;
}
.layui-layout-admin .c-side-menu .layui-nav-item{
	width:auto;
}
.layui-layout-admin .layui-nav.c-header-nav{
	float:right;
}
.layui-layout-admin .layui-nav.c-header-nav .layui-nav-item a{
	color:#333!important;
}
.layui-layout-admin .layui-nav.c-header-nav .layui-nav-item{

}
/*框架->左侧栏*/
.layui-layout-admin .layui-side{
	background-color:#ffffff!important;;
	box-shadow:5px 0 5px #F5F6FA;
	z-index:1001;
	width: 160px;
}
.layui-side-scroll,.layui-nav-tree{
	width: 160px;
}
.layui-layout-admin .c-side-menu {
	color:#ffffff;
}
.layui-layout-admin .c-side-menu .layui-nav-item a{
	color:#ffffff;
	padding:0 15px;
}

.layui-layout-admin .c-side-menu .layui-nav-item .layui-nav-child a{
	color:#555555;
	text-indent: 12px;
}
.layui-layout-admin .c-side-menu .layui-nav-item {
	padding:0 10px;
}
.layui-layout-admin .c-side-menu{
	background-color:#ffffff;
}
.layui-layout-admin .c-side-menu .layui-nav-itemed>.layui-nav-child{
	background-color:#ffffff!important;
}
.layui-layout-admin .c-side-menu .c-side-menu .layui-this{
	background-color:#F8F9FC!important;
}
.layui-layout-admin .c-side-menu .layui-this>a, .c-side-menu .layui-this>a:hover{
	background-color:#F8F9FC;
}
.layui-layout-admin .c-side-menu .layui-nav-item a{
	color:#333333!important;
}
.layui-layout-admin .c-side-menu .layui-nav-item.layui-this a{
	color:#3D82F7!important;
}
.layui-layout-admin .c-side-menu .layui-nav-item a:hover{
	background-color: #F8F9FC;
	color: #3d82f7!important;
}
.layui-layout-admin .c-side-menu .layui-nav-child a{
	color:#555555!important;
}
.layui-layout-admin .layui-nav .layui-nav-child dd.layui-this a, .layui-nav-child dd.layui-this{
	background-color:#F8F9FC!important;
	color:#3D82F7!important;
}
.layui-layout-admin .layui-nav .layui-nav-item a.layui-this {
	background-color: #F8F9FC!important;
	color: #3D82F7!important;
}
.layui-layout-admin .c-side-menu .layui-nav-bar{
	background-color:#F8F9FC!important;
	display:none;
}

.layui-layout-admin .layui-nav .layui-nav-mored, .layui-nav-itemed>a .layui-nav-more{
	margin-top: -9px;
	border-style: dashed dashed solid;
	border-color: transparent transparent #dedede!important;
}
.layui-layout-admin .layui-nav .layui-nav-more{
	border-color: #dedede transparent transparent;
}
.layui-nav-tree .layui-nav-child dd.layui-this, .layui-nav-tree .layui-nav-child dd.layui-this a, .layui-nav-tree .layui-this, .layui-nav-tree .layui-this>a, .layui-nav-tree .layui-this>a:hover{
	background-color: transparent;
	color:#ffffff;
}

